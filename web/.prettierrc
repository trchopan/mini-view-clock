{
  "printWidth": 100,
  "tabWidth": 4,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": false,
  "semi": false,
  "useTabs": false,
  "proseWrap": "preserve",
  "arrowParens": "avoid",
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [{"files": "*.svelte", "options": {"parser": "svelte"}}]
}
